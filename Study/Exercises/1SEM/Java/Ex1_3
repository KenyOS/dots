import java.util.Scanner;

public class Ex1_3 
{

	public static void main(String[] args) 
	{
		double r;
		
		Scanner reader;
		reader = new Scanner(System.in);
		System.out.print("Value of R: ");
		r = reader.nextDouble();
		
		//Area
		
		System.out.println(Math.PI*Math.pow(r, 2));
		reader.close();
	}

}
